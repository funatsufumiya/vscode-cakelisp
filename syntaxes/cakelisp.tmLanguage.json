    },
    "longstring": {
      "begin": "(@?)(`+)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.string.begin.cakelisp"
        },
        "2": {
          "name": "punctuation.definition.string.begin.cakelisp"
        }
      },
