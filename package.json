{
	"name": "vscode-cakelisp",
	"displayName": "Cakelisp",
	"description": "Cakelisp language support for Visual Studio Code",
	"icon": "images/icon.png",
	"version": "0.0.1",
	"publisher": "funatsufumiya",
	"repository": "https://github.com/funatsufumiya/vscode-cakelisp",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "cakelisp",
				"aliases": [
					"Cakelisp",
					"cakelisp"
				],
				"extensions": [
					".cake"
				],
				"configuration": "./cakelisp.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "cakelisp",
				"scopeName": "source.cakelisp",
				"path": "./syntaxes/cakelisp.tmLanguage.json"
			}
		],
		"commands": [
		],
		"keybindings": [
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^18.16.15",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.2.2"
	}
}
